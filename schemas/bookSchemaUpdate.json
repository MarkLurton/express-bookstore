{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "properties": {
    "isbn": {
      "type": "string",
      "default": "",
      "title": "The isbn Schema",
      "examples": ["asgfsafs"]
    },
    "amazon_url": {
      "type": "string",
      "default": "",
      "title": "The amazon_url Schema",
      "examples": ["google.com"],
      "format": "uri",
      "pattern": "^(.*)$"
    },
    "author": {
      "type": "string",
      "default": "",
      "title": "The author Schema",
      "examples": ["afsd"]
    },
    "language": {
      "type": "string",
      "default": "",
      "title": "The language Schema",
      "examples": ["asfasdf"]
    },
    "pages": {
      "type": "integer",
      "default": 0,
      "title": "The pages Schema",
      "examples": [250]
    },
    "publisher": {
      "type": "string",
      "default": "",
      "title": "The publisher Schema",
      "examples": ["dsfsdf"]
    },
    "title": {
      "type": "string",
      "default": "",
      "title": "The title Schema",
      "examples": ["dsfsdat"]
    },
    "year": {
      "type": "integer",
      "default": 0,
      "title": "The year Schema",
      "examples": [2022]
    }
  },
  "examples": [
    {
      "isbn": "asgfsafs",
      "amazon_url": "google.com",
      "author": "afsd",
      "language": "asfasdf",
      "pages": 250,
      "publisher": "dsfsdf",
      "title": "dsfsdat",
      "year": 2022
    }
  ],
  "additionalProperties": false
}
